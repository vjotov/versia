 <navigation-rule>
  <display-name>login</display-name>
  <from-view-id>/pages/login.xhtml</from-view-id>
  <!-- <navigation-case>
   <from-action>#{loginBean.CheckValidUser}</from-action>
   <from-outcome>success</from-outcome>
   <to-view-id>/pages/01_product.xhtml</to-view-id>
   <redirect/>
  </navigation-case> -->
  <navigation-case>
   <from-action>#{loginBean.CheckValidUser}</from-action>
   <from-outcome>success</from-outcome>
   <to-view-id>/pages/01_prod_rel_wss.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/01_product.xhtml</from-view-id>
  <navigation-case>
   <from-action>#{productBean.openProduct}</from-action>
   <from-outcome>open_release</from-outcome>
   <to-view-id>/pages/02_releases.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/02_releases.xhtml</from-view-id>
  <navigation-case>
   <from-action>#{userSessionBean.closeProduct}</from-action>
   <from-outcome>open_product</from-outcome>
   <to-view-id>/pages/01_product.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-action>#{releaseBean.openRelease}</from-action>
   <from-outcome>open_workspace</from-outcome>
   <to-view-id>/pages/03_open_workspace.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/03_open_workspace.xhtml</from-view-id>
  <navigation-case>
   <from-action>#{userSessionBean.closeRelease}</from-action>
   <from-outcome>open_release</from-outcome>
   <to-view-id>/pages/02_releases.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-action>#{wSListBean.openWorkspace}</from-action>
   <from-outcome>open_workspace</from-outcome>
   <to-view-id>/pages/04_workspace.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/04_workspace.xhtml</from-view-id>
  <navigation-case>
   <from-action>#{closeWorkspaceBean.closeWorkspace}</from-action>
   <from-outcome>workspace_closed</from-outcome>
   <to-view-id>/pages/01_prod_rel_wss.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-action>#{listVobjectsBean.showHistory}</from-action>
   <from-outcome>show_history</from-outcome>
   <to-view-id>/pages/05_history.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-action>#{listVobjectsBean.showDistribution}</from-action>
   <from-outcome>show_distribution</from-outcome>
   <to-view-id>/pages/06_distribution.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-action>#{showWorkitemsBean.showWorkitems}</from-action>
   <from-outcome>show_woritems</from-outcome>
   <to-view-id>/pages/07_workitems.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/05_history.xhtml</from-view-id>
  <navigation-case>
   <from-action>#{objectHistoryBean.showWorkspace}</from-action>
   <from-outcome>show_workspace</from-outcome>
   <to-view-id>/pages/04_workspace.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/06_distribution.xhtml</from-view-id>
  <navigation-case>
   <from-action>#{objectDistributionBean.showWorkspace}</from-action>
   <from-outcome>show_workspace</from-outcome>
   <to-view-id>/pages/04_workspace.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/07_workitems.xhtml</from-view-id>
  <navigation-case>
   <from-action>#{showWorkitemsBean.showWorkspace}</from-action>
   <from-outcome>show_workspace</from-outcome>
   <to-view-id>/pages/04_workspace.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>openWorkspace</display-name>
  <from-view-id>/pages/01_prod_rel_wss.xhtml</from-view-id>
  <navigation-case>
   <from-action>#{prwBean.openWorkSpace}</from-action>
   <from-outcome>open</from-outcome>
   <to-view-id>/pages/04_workspace.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
 </application>
